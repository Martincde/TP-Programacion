using System;

public class ProductoNumeros
{
    public static void Main(string[] args)
    {
        // Define los dos n√∫meros
        int numero1 = 12;
        int numero2 = 13;

        // Calcula el producto
        int resultado = numero1 * numero2;

        // Muestra el resultado en la consola
        Console.WriteLine("Calculando el producto de " + numero1 + " y " + numero2 + "...");
        Console.WriteLine("El resultado del producto es: " + resultado);

        // Opcional: Pausa la consola para que puedas ver el resultado antes de que se cierre
        Console.WriteLine("\nPresiona cualquier tecla para salir.");
        Console.ReadKey();
    }
}
