using System;

public class SumaGrande
{
    public static void Main(string[] args)
    {
        // Define los dos n√∫meros a sumar
        int numero1 = 12345;
        int numero2 = 67890;

        // Realiza la suma
        int resultado = numero1 + numero2;

        // Muestra el resultado en la consola
        Console.WriteLine("Calculando la suma de " + numero1 + " y " + numero2 + "...");
        Console.WriteLine("El resultado de la suma es: " + resultado);

        // Opcional: Pausa la consola para que puedas ver el resultado antes de que se cierre
        Console.WriteLine("\nPresiona cualquier tecla para salir.");
        Console.ReadKey();
    }
}
